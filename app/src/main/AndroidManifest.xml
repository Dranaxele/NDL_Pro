<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookiebots.ndlpro" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cookiebots.ndlpro.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cookiebots.ndlpro.StockActivity"
            android:label="@string/title_activity_stock" >
        </activity>
        <activity
            android:name="com.cookiebots.ndlpro.RefugieActivity"
            android:label="@string/title_activity_refugie" >
        </activity>
        <activity
            android:name="com.cookiebots.ndlpro.AjoutRefugieActivity"
            android:label="@string/title_activity_ajout_refugie" >
        </activity>
        <activity
            android:name="com.cookiebots.ndlpro.RechercheActivity"
            android:label="@string/title_activity_recherche" >
        </activity>
    </application>

</manifest>
